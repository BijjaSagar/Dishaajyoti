{
  "name": "dishaajyoti/api",
  "description": "DishaAjyoti Backend API",
  "type": "project",
  "require": {
    "php": ">=8.0",
    "ext-pdo": "*",
    "ext-json": "*",
    "ext-curl": "*",
    "firebase/php-jwt": "^6.0",
    "vlucas/phpdotenv": "^5.5",
    "guzzlehttp/guzzle": "^7.5",
    "ramsey/uuid": "^4.7"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "scripts": {
    "test": "phpunit"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
